func onPress40() {
  globalPress(40);

  dispatchToLayer(@onPress40_0, @onPress40_1, @onPress40_2, @onPress40_3, 0, @onPress40_5, @onPress40_6, @onPress40_7);
}
func onRelease40() {
  globalRelease();

  dispatchToLayer(@onRelease40_0, @onRelease40_1, @onRelease40_2, @onRelease40_3, 0, @onRelease40_5, @onRelease40_6, @onRelease40_7);
}

func onPress40_0() {
  pressStenoKeyButton(15 /* "U" */);
}
func onRelease40_0() {
  releaseStenoKey(15 /* "U" */);
}

func onPress40_1() {
  updateQwertyActionTimes();
  deactivateGlobal();
  deactivate1();
  activate2();
  activateGlobal();
}
func onRelease40_1() {
}

func onPress40_2() {
}
func onRelease40_2() {
  deactivateGlobal();
  deactivate2();
  activate1();
  activateGlobal();
  onRelease40_1();
}

func onPress40_3() {
  deactivateGlobal();
  deactivate3();
  activate5();
  activateGlobal();
}
func onRelease40_3() {
}

func onPress40_5() {
}
func onRelease40_5() {
  //
  // Since Settings -> Scripts -> deactivateLayer calls CallAllReleaseScripts,
  // this prevents infinite recursion.
  if (isInReleaseAll()) return;

  deactivateGlobal();
  deactivate5();
  activate3();
  activateGlobal();
  onRelease40_3();

}

