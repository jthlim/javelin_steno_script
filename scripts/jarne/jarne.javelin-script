func onPress38() {
  globalPress(38);

  dispatchToLayer(@onPress38_0, @onPress38_1, @onPress38_2, 0, @onPress38_4, 0, @onPress38_6, 0);
}
func onRelease38() {
  globalRelease();

  dispatchToLayer(@onRelease38_0, @onRelease38_1, @onRelease38_2, 0, @onRelease38_4, 0, @onRelease38_6, 0);
}

func onPress38_0() {
  pressStenoKeyButton(9 /* "O" */);
}
func onRelease38_0() {
  releaseStenoKey(9 /* "O" */);
}

var timer38_1;
var forceQwertySpace38;

func onPress38_1() {
  updateQwertyActionTimes();
  startTimer(104, 200, 0, @{
    if (layer != 1) return;

    if (!isButtonPressed(38)) return;

    //
    // This logic is used to set the layer when held
    if (!forceQwertySpace38) return;
    if (onPressScanCodeCallback == @tapSpace) {
      onPressScanCodeCallback = 0;
    }

    deactivateGlobal();
    deactivate1();
    activate4();
    activateGlobal();

  });
  timer38_1 = getTime();
  @{
    //
    // This key has a bunch of extra logic to it to make it behave
    // reliably like a space bar during fast typing.
    //
    // * If pressed within 100ms of pressing another key, then it will
    //   be treated as a spacebar unless held for 150ms (Hold Trigger).
    //
    // * If pressed without any other key, it will immediately switch to
    //   the navigation layer, after which fuzzy layer logic will take
    //   over to prevent

    // Suppress Layer Change if anything else was pressed in the last 200ms and it isn't temporary qwerty
    forceQwertySpace38 = !wasSteno24 && !isButtonPressed(37) && (getTime() - lastQwertyActionTimes[0] < 100);
    if (forceQwertySpace38) {
      // In this case, if another key is pressed, tap space.
      // In the hold trigger, this is removed.
      onPressScanCodeCallback = @tapSpace;

      // Prevent SetLayer
      return;
    }

    deactivateGlobal();
    deactivate1();
    activate4();
    activateGlobal();
    //
    startFuzzyLayer();

  }();

}
func onRelease38_1() {
  stopTimer(104);

  if (getTime() - timer38_1 <= 200) {
    @{
      //
      // Prevent space being tapped after it's already been sent.
      if (forceQwertySpace38 && onPressScanCodeCallback == 0) {
        return;
      }
      if (onPressScanCodeCallback == @tapSpace) {
        onPressScanCodeCallback = 0;
      }
      pressScanCodeButton(38, 0, 44 /* "⎵" */, 1);
      releaseScanCodeButton(44 /* "⎵" */);

    }();

  }
}

var timer38_2;
func onPress38_2() {
  timer38_2 = getTime();
  pressScanCodeButton(38, 0, 224 /* "⌃" */, 0);
}
func onRelease38_2() {
  releaseScanCodeButton(224 /* "⌃" */);
  if (getTime() - timer38_2 <= 200) {
    pressScanCodeButton(38, 0, 44 /* "⎵" */, 1);
    releaseScanCodeButton(44 /* "⎵" */);

  }
}

func onPress38_4() {
}
func onRelease38_4() {
  //
  stopFuzzyLayerPreSetLayer();
  deactivateGlobal();
  deactivate4();
  activate1();
  activateGlobal();
  onRelease38_1();
  //
  stopFuzzyLayerPostSetLayer();

}

func onPress38_6() {
}
func onRelease38_6() {
  deactivateGlobal();
  deactivate6();
  activate1();
  activateGlobal();
  onRelease38_1();
}

func onPress39() {
  globalPress(39);

  dispatchToLayer(@onPress39_0, @onPress39_1, 0, 0, @onPress39_4, 0, @onPress39_6, 0);
}
func onRelease39() {
  globalRelease();

  dispatchToLayer(@onRelease39_0, @onRelease39_1, 0, 0, @onRelease39_4, 0, @onRelease39_6, 0);
}

func onPress39_0() {
  pressStenoKeyButton(14 /* "E" */);
