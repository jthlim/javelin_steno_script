func activateDynamic() {
  dispatchToLayer(@activate0, @activate1, @activate2, @activate3, @activate4, @activate5, @activate6, @activate7);
}
func activateGlobal() {
  if (([[00 01 01 01 01 00 01 00]][resolveLayerIndex()])) {
    pressAll();
  }
  updateDisplay();
  rgbUpdate();
  rgbUpdateUnderglow();

}
