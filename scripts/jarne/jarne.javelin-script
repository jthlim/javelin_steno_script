  globalRelease();

  handleModTapRelease(@modTapHandler41);
  dispatchToLayer([[[@onRelease41_0, @onRelease41_1, 0, @onRelease41_3, 0, @onRelease41_5, @onRelease41_6, 0]]]);
}

func onPress41_0() {
  pressStenoKeyButton(27 /* "#2" */);
}
func onRelease41_0() {
  releaseStenoKey(27 /* "#2" */);
}

func onPress41_1() {
  modTapLayerPossibility[3] = 1;
  handleModTapPress(@modTapHandler41, 16, modTapLayerActivations41, MODTAP_DEFAULT_MODTAP_HOLD_TIME);
}
func onRelease41_1() {
  modTapLayerPossibility[1] = 0;
  modTapLayerPossibility[3] = 0;
}

func onPress41_2() {
  deactivateGlobal();
  deactivate2();
  activate5();
  activateGlobal();
}
func onRelease41_2() {
}

func onPress41_3() {
}
func onRelease41_3() {
  deactivateGlobal();
  deactivate3();
  activate1NoRelease();
  activateGlobal();
  onRelease41_1();
}

func onPress41_4() {
  handleModTapPress(@modTapHandler41, 16, modTapLayerActivations41, MODTAP_DEFAULT_MODTAP_HOLD_TIME);
}
func onRelease41_4() {
}

func onPress41_5() {
}
func onRelease41_5() {
  //
  // Since Settings -> Scripts -> deactivateLayer calls CallAllReleaseScripts,
  // this prevents infinite recursion.
  if (isInReleaseAll()) return;

  deactivateGlobal();
  deactivate5();
  activate2();
  activateGlobal();
  onRelease41_2();

}

func onPress41_6() {
  pressScanCodeButton(41, 0, 99 /* "." */, 0);
}
func onRelease41_6() {
  releaseScanCodeButton(99 /* "." */);
}

