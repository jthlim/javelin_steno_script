func onPress1_5() {
  startTimer(100, 2000, 0, @{
    if (layer != 5) return;

    if (!isButtonPressed(1)) return;

    // Hold to Pair
    startPairing();

  });
  // BLE 1
  setPreferredConnection(CONNECTION_BLE, CONNECTION_USB, CONNECTION_USB2);
  setBleProfile(0);
  drawBleSummary();

}
func onRelease1_5() {
  stopTimer(100);
}

