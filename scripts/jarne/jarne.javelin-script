func onPress41() {
  globalPress(41);

  dispatchToLayer(@onPress41_0, @onPress41_1, @onPress41_2, @onPress41_3, @onPress41_4, @onPress41_5, @onPress41_6, 0);
}
func onRelease41() {
  globalRelease();

  dispatchToLayer(@onRelease41_0, @onRelease41_1, @onRelease41_2, @onRelease41_3, @onRelease41_4, @onRelease41_5, @onRelease41_6, 0);
}

func onPress41_0() {
  pressStenoKeyButton(27 /* "#2" */);
}
func onRelease41_0() {
  releaseStenoKey(27 /* "#2" */);
}

var timer41_1;
var forceQwertyEnter41;
func onPress41_1() {
  updateQwertyActionTimes();
  timer41_1 = getTime();
  @{
    //
    // Suppress Layer Change if anything else was pressed in the last 150ms and it isn't temporary qwerty
    forceQwertyEnter41 = !wasSteno24 && !isButtonPressed(40) && (getTime() - lastQwertyActionTimes[0] < 150);
    if (forceQwertyEnter41) {
      pressScanCode(40 /* Enter */);
      return;
    }

    deactivateGlobal();
    deactivate1();
    activate3();
    activateGlobal();
    //
    startFuzzyLayer();

  }();

}
func onRelease41_1() {
  //
  if (forceQwertyEnter41) {
    releaseScanCode(40 /* Enter */);
    return;
  }

  if (getTime() - timer41_1 <= 200) {
    pressScanCodeButton(41, 0, 40 /* "↵" */, 1);
    releaseScanCodeButton(40 /* "↵" */);

  }
}

func onPress41_2() {
  deactivateGlobal();
  deactivate2();
  activate5();
  activateGlobal();
}
func onRelease41_2() {
}

func onPress41_3() {
}
func onRelease41_3() {
  //
  stopFuzzyLayerPreSetLayer();
  deactivateGlobal();
  deactivate3();
  activate1();
  activateGlobal();
  onRelease41_1();
  //
  stopFuzzyLayerPostSetLayer();

}

func onPress41_4() {
  pressScanCodeButton(41, 0, 40 /* "↵" */, 0);
}
func onRelease41_4() {
  releaseScanCodeButton(40 /* "↵" */);
}

func onPress41_5() {
}
func onRelease41_5() {
  //
  // Since Settings -> Scripts -> deactivateLayer calls CallAllReleaseScripts,
  // this prevents infinite recursion.
  if (isInReleaseAll()) return;

  deactivateGlobal();
  deactivate5();
  activate2();
  activateGlobal();
  onRelease41_2();

}

