  activate4();
  activateGlobal();
}
func onRelease41_1() {
}

func onPress41_2() {
}
func onRelease41_2() {
  deactivateGlobal();
  deactivate2();
  activate0NoRelease();
  activateGlobal();
  onRelease41_0();
}

func onPress41_3() {
  handleModTapPress(@modTapHandler41, 16, modTapLayerActivations41, MODTAP_DEFAULT_MODTAP_HOLD_TIME);
}
func onRelease41_3() {
}

func onPress41_4() {
}
func onRelease41_4() {
  //
  // Since Settings -> Scripts -> deactivateLayer calls CallAllReleaseScripts,
  // this prevents infinite recursion.
  if (isInReleaseAll()) return;

  deactivateGlobal();
  deactivate4();
  activate1();
  activateGlobal();
  onRelease41_1();
}

func onPress41_5() {
  pressScanCodeButton(41, 0, 99 /* "." */, 0);
}
func onRelease41_5() {
  releaseScanCodeButton(99 /* "." */);
}
