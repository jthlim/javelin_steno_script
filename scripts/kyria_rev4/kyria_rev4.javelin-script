var pressTime42;
func onPress42() {
  globalPress(42);

  if (!isInPressAll()) pressTime42 = getTime();
  dispatchToLayer([[[@onPress42_0, @onPress42_1, @onPress42_2, 0, 0, 0]]]);
}
func onRelease42() {
  globalRelease();

  dispatchToLayer([[[@onRelease42_0, @onRelease42_1, @onRelease42_2, 0, 0, 0]]]);
}

