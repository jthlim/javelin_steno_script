func onPress6_5() {
  // Speed Up
  if (speed < 8) {
    hueShift = hueShift + getTime() * (SPEED_VALUES[speed] - SPEED_VALUES[speed + 1]);
    speed = speed + 1;
    rgbUpdateRandomColors();
    drawBar(SPEED_IMAGE, speed);
  }
}
func onRelease6_5() {
}

