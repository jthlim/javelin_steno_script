  pressStenoKeyButton(8 /* "A" */);
}
func onRelease42_0() {
  releaseStenoKey(8 /* "A" */);
}

func onPress42_1() {
  pressScanCodeButton(42, 0, 226 /* "âŒ¥" */, 0);
}
func onRelease42_1() {
  releaseScanCodeButton(226 /* "âŒ¥" */);
  if (getTime() - pressTime42 <= 200) {
    pressScanCodeButton(42, 0, 40 /* "â†µ" */, 0);

  }
}

