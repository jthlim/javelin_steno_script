var pressTime12;
func onPress12() {
  globalPress(12);

  if (!isInPressAll()) pressTime12 = getTime();
  dispatchToLayer([[[0, @onPress12_1, @onPress12_2, 0, 0, 0]]]);
}
func onRelease12() {
  globalRelease();

  dispatchToLayer([[[0, @onRelease12_1, @onRelease12_2, 0, 0, 0]]]);
}

func onPress12_1() {
  pressScanCodeButton(12, 0, 224 /* "âŒƒ" */, 0);
}
func onRelease12_1() {
  releaseScanCodeButton(224 /* "âŒƒ" */);
  if (getTime() - pressTime12 <= 200) {
    pressScanCodeButton(12, 0, 41 /* "âŽ‹" */, 1);
    releaseScanCodeButton(41 /* "âŽ‹" */);

  }
}

