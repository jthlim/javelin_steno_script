func onPress12_1() {
  pressScanCodeButton(224 /* "‚åÉ" */, 0);
}
func onRelease12_1() {
  releaseScanCode(224 /* "‚åÉ" */);
  if (getTime() - pressTime12 <= 200) {
    pressScanCodeButton(41 /* "‚éã" */, 1);

    releaseScanCode(41 /* "‚éã" */);
  }
}

