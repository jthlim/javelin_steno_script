var pressTime24;
func onPress24() {
  if (!isInPressAll()) pressTime24 = getTime();
  dispatchToLayer([<@onPress24_0, @onPress24_1, 0, 0, 0>]);
}
func onRelease24() {
  dispatchToLayer([<0, @onRelease24_1, 0, 0, 0>]);
}

func onPress24_0() {
  deactivateGlobal();
  deactivate0();
  activate1();
  activateGlobal();
}
func onRelease24_0() {
}

var pressCount24_1;
var releaseCount24_1;
func onPress24_1() {
  pressScanCodeButton(225 /* "‚áß" */, 0);

  if (!isInPressAll()) {
    pressCount24_1 = getPressCount();
    releaseCount24_1 = getReleaseCount();
  }
}
func onRelease24_1() {
  releaseScanCode(225 /* "‚áß" */);
  if (getTime() - pressTime24 <= 200 && pressCount24_1 == getPressCount() && releaseCount24_1 + 1 == getReleaseCount()) {
    deactivateGlobal();
    deactivate1();
    activate0();
    activateGlobal();
  }
}

